package firstproject;

public class whileloop {
	public static void main(String[]args) {
		int i=1;//initialisation
		while(i<=10)//condition
		{
			System.out.println(i);
			i++;//increement
		}
		int j=0;//initi
		do
		{
			System.out.println("DO WHILE LOOP");
			j++;//incr
		}while(j>5);//cond
		}
	}
